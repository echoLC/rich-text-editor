{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs", 
    "outDir": "dist", 
    "noImplicitAny": true,
    "strict": true,
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./src/*"],
  "exclude": [
    "node_modules",
    "dist",
    "test",
    "examples"
  ]
}


